   "source": [
    "def solution(array, commands):\n",
    "    answer = []\n",
    "    \n",
    "    for a, b, c in commands:\n",
    "        cut = array.copy()\n",
    "        cut = cut[a-1:b]\n",
    "        cut = sorted(cut)\n",
    "        answer.append(cut[c-1])\n",
    "    return answer"
   ]
  }
